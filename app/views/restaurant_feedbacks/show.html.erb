<%- model_class = RestaurantFeedback -%>
<div class="page-header">
  <h1><%=t '.title', :default => RestaurantFeedback %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:content) %></dt>
      <dd><pre class="prettyprint"><%= @restaurant_feedback.content %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              restaurant_feedbacks_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_restaurant_feedback_path(@restaurant_feedback), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              restaurant_feedback_path(@restaurant_feedback),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

