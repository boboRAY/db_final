<h1>Listing driver_feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Driver</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @driver_feedbacks.each do |driver_feedback| %>
      <tr>
        <td><%= driver_feedback.description %></td>
        <td><%= driver_feedback.driver %></td>
        <td><%= driver_feedback.user %></td>
        <td><%= link_to 'Show', driver_feedback %></td>
        <td><%= link_to 'Edit', edit_driver_feedback_path(driver_feedback) %></td>
        <td><%= link_to 'Destroy', driver_feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Driver feedback', new_driver_feedback_path %>
